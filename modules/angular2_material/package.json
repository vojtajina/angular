    "rxjs": "<%= packageJson.dependencies['rxjs'] %>",
    "zone.js": "<%= packageJson.dependencies['zone.js'] %>"
  },
  "devDependencies": <%= JSON.stringify(packageJson.defaultDevDependencies) %>
  "peerDependencies": {
    "angular2": "<%= packageJson.version %>"
  }
